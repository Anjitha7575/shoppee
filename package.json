    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^6.1.1",
    "jest": "^27.5.1",
    "react-testing-library": "^8.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/apps/CommonTemplates/*",
      "src/apps/**/index.js",
      "!src/constants/*",
      "!src/apps/**/reducer/*",
      "!src/apps/**/actions/*"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 80,
        "functions": 80,
        "lines": 80
      }
    }
  }
